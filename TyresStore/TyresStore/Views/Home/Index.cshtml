@model IList<TyresStore.Repository.Models.Vehicle>

@{
    ViewBag.Title = "Home Page";
}


<h1> Welcome to our store!</h1>

<p style="padding-top: 30px; padding-bottom: 30px;">In order to find the right tire for your car, please select your car's brand from the below table.<br></p>

@if (Model.Count > 0)
{
    <div class="table">
        <div class="header-table"> Vehicle </div>
        <table>
            <thead>
                <tr>
                    <th>Manufacter</th>
                    <th>Model</th>
                    <th>Year</th>
                    <th>Color</th>
                    <th>Id</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var Vehicle in Model)
                {
                    <tr>
                        <td>@Vehicle.Manufacturer</td>
                        <td>@Vehicle.Model</td>
                        <td>@Vehicle.Year</td>
                        <td>@Vehicle.Color</td>
                        <td>@Vehicle.ID</td>
                        <td class="button-select">
                            <button type="button" class="btn btn-default btn-md" onclick="mainModel.loadTyres(@Vehicle.ID)">  Select to see tyres <span class="glyphicon glyphicon-plus-sign"></span></button>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}

<div class="tyre-table">
    <div class="table">
        <div class="header-table-tyres"> Tyres </div>
        <div class="table-content"></div>
    </div>
</div>

<p style="padding-top: 100px; padding-bottom: 10px; text-align:center">
    Please like us on Facebook:
    <a href="https://www.facebook.com/dvse.ro/">
        <img src="data:image/jpeg;base64," alt="Go to DVSE.ro!" width="25" height="25" border="0">
    </a>
</p>




